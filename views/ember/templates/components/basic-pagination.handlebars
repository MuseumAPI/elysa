{{#if pages}}
 <ul class="pagination pagination-sm">
  <li {{bind-attr class='onFirstPage:disabled'}}><a href="#" {{action 'gotoPrev'}}><span aria-hidden="true">&laquo;</span><span class="sr-only">Previous</span></a></li>
  {{#each page in pages}} 
     <li {{bind-attr class='page.isCurrent:active'}}>
     {{#if page.placeholder}}
        <a href="#">...</a>
      {{else}}
        <a href="#" {{action 'gotoPage' page.pageNumber}}>{{page.pageNumber}}</a>
      {{/if}}
     </li>
  {{/each}}
  <li {{bind-attr class='onLastPage:disabled'}}><a href="#" {{action 'gotoNext'}}><span aria-hidden="true">&raquo;</span><span class="sr-only">Next</span></a></li>
</ul>
{{else}}
<h5> No Results Found.</h5>
{{/if}}